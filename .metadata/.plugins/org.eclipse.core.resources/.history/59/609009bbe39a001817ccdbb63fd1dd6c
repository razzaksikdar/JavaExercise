package accountsystem;

import java.util.*;
public class Accountservices {
	
private HashMap<String,Accountdetails> accounts;

public Accountservices() {
	
	setAccounts(new HashMap<String, Accountdetails>());
	
}

public void addaccounts(String key, Accountdetails account) {
	this.getAccounts().put(key, account);
	
}

public Accountdetails getAccount(String key) {

  return this.getAccounts().get(key);
 


}

public HashMap<String,Accountdetails> getAccounts() {
	return accounts;
}

public void setAccounts(HashMap<String,Accountdetails> accounts) {
	this.accounts = accounts;
}


}



