package accountsystem;

public class Systemfunction {
	Accountservices run;
	
	public Systemfunction() {
		run = new Accountservices();
	    run.addaccounts("1",new Accountdetails("bob","dylan","3255" ));
		run.addaccounts("2", new Accountdetails("Mike","Mathers","54545"));
		
	}
	public void printbyKey(String key) {
		Accountdetails accountd = run.getAccount("2");
		System.out.println(accountd);
	}
	
	public void key() {
		this.printbyKey("2");
		
	}
	
	

}
